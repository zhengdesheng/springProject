<?xml version="1.0" encoding="UTF-8" ?>
<ehcache name="hibernateCache" updateCheck="false">
	<defaultCache maxElementsInMemory="100" eternal="false"
		overflowToDisk="true" maxEntriesLocalHeap="100" timeToIdleSeconds="100"
		timeToLiveSeconds="100" maxEntriesLocalDisk="100000" />
	<cache name="com.exam.mserver.entity.User" eternal="false"
		overflowToDisk="true" maxEntriesLocalHeap="100" maxEntriesLocalDisk="1000" />
		
	<cache name="org.hibernate.cache.UpdateTimestampsCache"
		maxElementsInMemory="5000" eternal="true" overflowToDisk="true" />

	<cache name="org.hibernate.cache.StandardQueryCache"
		maxElementsInMemory="10000" eternal="false" timeToLiveSeconds="120"
		overflowToDisk="true" />
</ehcache>